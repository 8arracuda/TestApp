<?xml version='1.0' encoding='utf-8'?>
<widget id="de.msteiner.TestApp" version="0.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <preference name="Fullscreen" value="true" />
    <feature name="Camera">
        <param name="browser-package" value="Camera" />
    </feature>
    <name>Test-App</name>
    <description>
        Test-App with MobileAngularUI and AngularJS
    </description>
    <author email="dev@cordova.apache.org" href="http://cordova.io">
        Apache Cordova Team
    </author>
    <content src="index.html" />
    <access origin="*" />
    <preference name="orientation" value="portrait" />
    <feature name="Device">
        <param name="browser-package" value="Device" />
    </feature>
</widget>
